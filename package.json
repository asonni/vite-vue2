  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^1.1.2",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.3.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "vite": "^3.0.7",
    "vite-plugin-eslint": "^1.8.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
